{
  "control_characters": {
    "0": "ignore",
    "5": "ignore",  // ENQ, Ctrl-E
    "7": "ignore",
    "8": "cub1",  // BS, Ctrl-H
    "9": "ht",  // HT, Ctrl-I
    "10": "ind",  // LF, Ctrl-J
    "11": "ind",  // VT, Ctrl-K
    "12": "ind",  // FF, Ctrl-L
    "13": "cr"  // CR, Ctrl-M
    // '14': 'ignore'
    // '15': 'ignore'
  },
  "escape_sequences": {
    "\\E7": "sc",
    "\\E8": "rc",
    "\\Ec\\E]R": "rs1",
    "\\EH": "ignore",
    "\\EM": "ri",
    "\\E]R": "ignore",
    "\\E[4h": "smir",
    "\\E[4l": "rmir",

    "\\E[0m": "default_rendition",
    "\\E[1m": "bold",
    "\\E[2m": "dim",
    "\\E[4m": "smul",
    "\\E[5m": "blink",
    "\\E[?7h": "ignore",
    "\\E[?25h\\E[?0c": "ignore",
    "\\E[?25l\\E[?1c": "ignore",
    "\\E[?1000h": "civis",
    "\\E[?1000l": "cvvis",
    "\\E[?2004h": "ignore",
    "\\E[?2004l": "ignore",

    "\\E[7m": "rev",  // is represented by both rev and smso
    "\\E[10m": "rmpch",
    "\\E[11m": "smpch",
    "\\E[24m": "rmul",
    "\\E[27m": "rmso",
    "\\E[0;10m": "sgr0",
    "\\E[39;49m": "op",
    "\\E[A": "kcuu1",
    "\\E[B": "kcud1",

    "\\E[C": "kcuf1",  // cuf1
    "\\E[D": "kcub1",
    "\\E[G": "kb2",
    "\\E[H": "home",
    "\\E[J": "ed",
    "\\E[K": "el",
    "\\E[1K": "el1",
    "\\E[L": "il1",
    "\\E[M": "dl1",
    "\\E[P": "dch1",

    "\\E]133;A": "ignore",  // shell sequence to be run before the command output (Mac OS X-specific)
    "\\E]133;B": "ignore",  // shell sequence to be run after the prompt (Mac OS X-specific)
    "\\E]133;C;": "ignore"  // shell sequence to be run before the command output (Mac OS X-specific)
  },
  "escape_sequences_re": {
    "\\E[%d@": "ich",
    "\\E[%dd": "vpa",
    "\\E[%dm": "set_color",
    "\\E[%d;%dm": "set_color_pair",
    "\\E[%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%d;%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%d;%d;%d;%d;%dm": "sgr",
    "\\E[%d;%d;%d;%d;%d;%d;%d;%d;%dm": "sgr",
    "\\E[%dB": "cud",
    "\\E[%dC": "cuf",
    "\\E[%dG": "hpa",
    "\\E[%dL": "il",
    "\\E[%dM": "dl",
    "\\E[%dP": "dch",
    "\\E[%dX": "ech",
    "\\E[%d;%dr": "csr",
    "\\E[%d;%dH": "cup",

    // Shell sequence to be run after the command output (Mac OS X-specific)
    // %d is the command status, 0-255.
    "\\E]133;D;%d": "ignore"
  }
}
